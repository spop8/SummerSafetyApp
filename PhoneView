import SwiftUI

struct PhoneView: View {
    var body: some View {
        ZStack {
            Color.secondary
                .ignoresSafeArea()
            Image("phone")
                .resizable()
                .scaledToFill()
            
        }    }
}

#Preview {
    PhoneView()
}
